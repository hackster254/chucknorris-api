te({\n      ...state,\n      joke: result.data.value,\n      searchedjoke: response.data.result[0].value,\n    });\n  }\n\n  const handleSearch = (e) => {\n    e.preventDefault()\n    fetchData();\n  }\n  // get categories\n  /*\n    const getCategories = async () => {\n      const results = await fetch(\n        \"https://api.chucknorris.io/jokes/categories\"\n      );\n      const categories = await results.json();\n      console.log(categories)\n      //this.setState({ categories });\n      setState({\n        categories: categories\n      })\n      \n    };\n    */\n\n\n  return (\n    <div className=\"container\">\n      <div className=\"row\">\n        <div className=\"col-6\">\n          <h1 className=\"title\">Chuck Norris Api</h1>\n          <img src={Chuck} alt=\"Chuck Norris\" />\n        </div>\n\n        <div className=\"col-6 searchJokeCol\">\n          <div className=\"card\">\n            <div className=\"card-header\">Search for a word</div>\n            <form onSubmit={handleSearch}>\n              <div className=\"card-body\">\n                <input\n                  type=\"text\"\n                  onChange={(e) => setQuery(e.target.value)}\n                  value={query}\n                />\n              </div>\n\n              <div>\n                <button type=\"submit\" className=\"btn btn-warning btn-lg\">\n                  GENERATE JOKE\n                </button>\n              </div>\n              \n            </form>\n\n              <div>\n                  <button className=\"btn btn-success btn-lg\">\n                    RANDOM\n                  </button>\n              </div>\n          </div>\n        </div>\n\n        <h2 className=\"subtitle\">Here is the Joke</h2>\n        <h4>{state.joke}</h4>\n        <break></break>\n        <h4>{state.searchedjoke}</h4>\n      </div>\n    </div>\n  );\n}\n\nexport default App;\n",{"ruleId":"15","replacedBy":"16"},{"ruleId":"17","replacedBy":"18"},{"ruleId":"19","severity":1,"message":